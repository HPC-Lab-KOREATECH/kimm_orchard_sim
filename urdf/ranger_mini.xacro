      <joint name="rl_steering_wheel">
        <command_interface name="position">
          <param name="min">-3.14</param>
          <param name="max">3.14</param>
        </command_interface>
        <state_interface name="position"/>
        <state_interface name="velocity"/>
        <state_interface name="effort"/>
      </joint>

      <joint name="rr_steering_wheel">
        <command_interface name="position">
          <param name="min">-3.14</param>
          <param name="max">3.14</param>
        </command_interface>
        <state_interface name="position"/>
        <state_interface name="velocity"/>
        <state_interface name="effort"/>
      </joint>
  </ros2_control>

  <!-- MACRO -->

  <xacro:macro name="box_inertia" params="m x y z">
      <inertia ixx="${m * (y * y + z * z) / 12}" ixy="0" ixz="0" iyy="${m * (x * x + z * z) / 12}" iyz="0" izz="${m * (y * y + x * x) / 12}" />
  </xacro:macro>

  <!-- LIDAR -->
  <xacro:include filename="$(find kimm_orchard_sim)/urdf/sensors/VLP-32.urdf.xacro"/>
  <xacro:VLP-32 >
    <origin xyz="0 0 0" rpy="-1.57 -1.57 0.0" />
  </xacro:VLP-32> 

  <!-- CAMERA -->
  <xacro:include filename="$(find kimm_orchard_sim)/urdf/sensors/kinect_camera.xacro" />
  <xacro:kinect_camera />

  <!-- IMY -->
  <xacro:include filename="$(find kimm_orchard_sim)/urdf/sensors/imu.xacro" />
  <xacro:imu />

  <gazebo>
    <plugin name="gazebo_ros2_control" filename="libgazebo_ros2_control.so">
      <parameters>$(find kimm_orchard_sim)/config/controller.yaml</parameters>
    </plugin>
  </gazebo>
</robot>

